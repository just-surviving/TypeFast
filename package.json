{
  "name": "typefast",
  "version": "1.0.0",
  "description": "A modern, real-time typing speed test application with multiplayer races, leaderboards, and analytics",
  "private": true,
  "author": "just-surviving",
  "license": "MIT",
  "homepage": "https://github.com/just-surviving/TypeFast#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/just-surviving/TypeFast.git"
  },
  "keywords": [
    "typing",
    "speed-test",
    "multiplayer",
    "real-time",
    "nextjs",
    "typescript",
    "websocket"
  ],
  "scripts": {
    "build": "cd apps/web/DB_prisma && prisma migrate deploy && prisma generate && cd ../ && next build",
    "prisma": "cd apps/web/DB_prisma && prisma migrate deploy && prisma generate",
    "start": "cd apps/web && next start",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint --fix",
    "db:setup": "cd apps/web/DB_prisma && prisma migrate deploy && prisma generate",
    "db:reset": "cd apps/web/DB_prisma && prisma migrate reset",
    "db:seed": "cd apps/web/DB_prisma && prisma db seed",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "type-check": "turbo type-check",
    "clean": "turbo clean"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "next-themes": "^0.4.6",
    "prisma": "^6.14.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript-eslint": "^8.22.0"
  }
}
